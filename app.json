{
  "expo": {
    "name": "biblioteca",
    "slug": "biblioteca",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5653D4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
	  "bundleIdentifier": "com.biasan.bibliotecaEletronica",
	  "buildNumber": "1.0.0",
    },
    "android": {
		"package": "com.biasan.bibliotecaEletronica",
		"versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5653D4"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
